{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "outDir": "../../out-tsc/app",
    "types": [],
    "paths": {
      "@app/*": [
        "app/core/*"
      ],
      "@shared/*": [
        "src/app/shared/*"
      ],
      "@env": [
        "environments/environment"
      ],
      "@modules/*": [
        "app/modules/*"
      ]
    },
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
